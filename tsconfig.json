{
  "compilerOptions": {
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "strict": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "moduleResolution": "node",
    "lib": ["ES2022"],
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@lib/*": ["example/lib/*"],
      "#grpc/*": ["grpc/_types/*"]
    }
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
